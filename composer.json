{
    "name": "uitccommerce/assets",
    "description": "Laravel assets management",
    "license": "MIT",
    "keywords": [
        "uitccommerce",
        "uitccommerce cms",
        "uitccommerce platform",
        "uitccommerce assets"
    ],
    "homepage": "https://uitccommerce.com",
    "authors": [
        {
            "name": "Uitccommerce",
            "email": "uitccommerce@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0|^8.1",
        "laravel/framework": "^5.6|^6|^7|^8|^9|^10"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1|^7.0|^8.0|^9.0|^10.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Uitccommerce\\Assets\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uitccommerce\\Assets\\Providers\\AssetsServiceProvider"
            ],
            "aliases": {
                "Assets": "Uitccommerce\\Assets\\Facades\\AssetsFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
